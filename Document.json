{
  "$jason": {
    "head": {
      "title": "Feed Database",
      "offline": "true",
      "styles": {
        "bold": {
          "font": "HelveticaNeue-CondensedBold",
          "size": "15"
        },
        "padded": {
          "padding": "10"
        },
        "normal": {
          "font": "HelveticaNeue",
          "size": "12"
        }
      },
      "actions": {
        "$foreground": {
          "type": "$reload"
        }
      }
    },
    "body": {
      "header": {
        "title": "Feed Database",
        "style": {
          "background": "#8bb92d",
          "color": "#ffffff",
          "font": "AvenirNext-Bold",
          "size": "14"
        }
      },
      "sections": [
        {
          "items": [
            {
              "type": "vertical",
              "style": {
                "background": "rgba(14,122,254,0.1)",
                "padding": "20"
              }
            }
          ]
        },
        {
          "header": {
            "type": "vertical",
            "style": {
              "background": "#ebebeb"
            },
            "components": [
              {
                "type": "label",
                "class": "bold",
                "text": "Species"
              },
              {
                "type": "label",
                "class": "normal",
                "text": "Select Species"
              }
            ]
          },
          "items": [
            {
              "type": "label",
              "text": "Chicken",
              "class": "bold padded",
              "href": {
                "url": "https://www.jasonbase.com/things/B8LP.json"
              }
            },
            {
              "type": "label",
              "text": "Place Holder",
              "class": "bold padded",
              "href": {
                "url": "https://www.jasonbase.com/things/B8LP.json"
              }
            },
            {
              "type": "label",
              "text": "Place Holder",
              "class": "bold padded",
              "href": {
                "url": "https://www.jasonbase.com/things/zMVo.json"
              }
            },
            {
              "type": "label",
              "text": "Place Holder",
              "class": "bold padded",
              "href": {
                "url": "https://jasonette.github.io/Jasonpedia/webcontainer/index.json"
              }
            }
          ]
        },
        {
          "header": {
            "type": "vertical",
            "style": {
              "background": "#ebebeb"
            },
            "components": [
              {
                "type": "label",
                "class": "bold",
                "text": "Showcase"
              },
              {
                "type": "label",
                "class": "normal",
                "text": "Place Holder"
              }
            ]
          },
          "items": [
            {
              "type": "label",
              "text": "Place Holder",
              "href": {
                "url": "https://raw.githubusercontent.com/Jasonette/Instagram-UI-example/master/home.json",
                "transition": "modal"
              }
            },
            {
              "type": "label",
              "text": "Placeholder",
              "href": {
                "url": "https://raw.githubusercontent.com/Jasonette/Twitter-UI-example/master/home.json",
                "transition": "modal"
              }
            }
          ]
        }
      ]
    }
  }
}