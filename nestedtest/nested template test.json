{
  "$jason": {
    "head": {
      "title": "Fatal Medication & Species Combination",
      "offline": "true",
      "actions": {
        "$load": {
          "type": "$network.request",
          "options": {
            "url": "https://raw.githubusercontent.com/twenty4k/Medicated-Feed-JASON-App/master/nestedtest/meds%20and%20mineral.json"
          },
          "success": {
            "type": "$render"
          }
        }
      },
      "templates": {
        "body": {
          "style": {
            "border": "none"
          },
          "header": {
            "title": "Fatal Medication & Species Combination",
            "menu": {
              "text": "21 CFR 558",
              "href": {
                "url": "http://api.fdsys.gov/link?collection=cfr&titlenum=21&partnum=558&link-type=xml",
                "view": "web"
              }
            }
          },
          "sections": [
            {
              "header": {
                "type": "label",
                "text": "These combinations may be fatal if consumed.  References include CPGM 7371.004, 21 CFR 558, and Mineral Tolerance of Animals Second Revised Edition 2005",
                "style": {
                  "background": "#f5f5f5",
                  "padding": "20"
                }
              },
              "items": {
                "{{#each Cattle}}": {
                  
                  "type": "vertical",
                  "style": {
                    "padding": "5",
                    "spacing": "5"
                  },
                  "components": [
                    {
                      "type": "label",
                      "style": {
                        "size": "14"
                      },
                      "text": "{{this}}"
                    },
                    
                 
                    {
                      "type": "space",
                      "style": {
                        "height": "20"
                      }
                    }
                  ]
                  
                }
              }
            }
          ]
        }
      }
    }
  }
}
