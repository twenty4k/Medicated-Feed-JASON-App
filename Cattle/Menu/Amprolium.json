{
  "$jason": {
    "head": {
      "title": "Cattle Amprolium",
      "offline": "true",
      "actions": {
        "$load": {
          "type": "$network.request",
          "options": {
            "url": "https://raw.githubusercontent.com/twenty4k/Medicated-Feed-JASON-App/master/Cattle/Amprolium.json"
          },
          "success": {
            "type": "$render"
          }
        }
      },
      "templates": {
        "body": {
          "style": {
            "border": "#000000"
          },
          "header": {
            "title": "Amprolium"
          },
          "sections": [
            {
              "header": {
                "type": "label",
                "text": "§ 558.55 Amprolium.&#40 a &#41 Approvals",
                "style": {
                  "background": "#f5f5f5",
                  "padding": "20"
                }
              },
              "items": {
                "{{#each $jason}}": {
                  "type": "vertical",
                  "style": {
                    "padding": "5",
                    "spacing": "5"
                  },
                  "components": [
                    {
                      "type": "label",
                      "style": {
                        "size": "14"
                      },
                      "text": "{{Cattle}}"
                    },
                    {
                      "type": "label",
                      "style": {
                        "size": "14"
                      },
                      "text": "Feeding Rate or Amount: {{Amount}}"
                    },
                    {
                      "type": "label",
                      "style": {
                        "size": "14"
                      },
                      "text": "Combination: {{Combination}}"
                    },
                    {
                      "type": "label",
                      "style": {
                        "size": "14"
                      },
                      "text": "Indications for use: {{Indicationsforuse}}"
                    },
                    {
                      "type": "label",
                      "style": {
                        "size": "14"
                      },
                      "text": "Limitations: {{Limitations}}"
                    },
                    {
                      "type": "label",
                      "style": {
                        "size": "14"
                      },
                      "text": "Sponsor: {{Sponsor}}"
                    },
                    {
                      "type": "space",
                      "style": {
                        "height": "50"
                      }
                    }
                  ],
                  "action": {
                    "type": "$widget.banner",
                    "options": {
                      "title": "You selected",
                      "description": "{{title}}"
                    }
                  }
                }
              }
            }
          ]
        }
      }
    }
  }
}
