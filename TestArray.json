{
  "$jason": {
    "head": {
      "title": "Test",
      "offline": "true",
      "actions": {
        "$load": {
          "type": "$network.request",
          "options": {
            "url": "https://raw.githubusercontent.com/twenty4k/Medicated-Feed-JASON-App/master/test.json"
          },
          "success": {
            "type": "$render"
          }
        }
      },
      "templates": {
        "body": {
          "style": {
            "border": "#000000"
          },
          "header": {
            "title": "Amprolium"
          },
          "sections": [{
              "header":{
                "type": "label",
                "text": "Weights are g/ton unless indicated otherwise",
                "style": {"background": "#f5f5f5","padding": "20"}
		  },
              "items": {
                "{{#each $Amprolium}}": {
                  "type": "label",
                  "text": "{{this}}"
                }
              }
            }
          ]
        }
      }
    }
  }
}
